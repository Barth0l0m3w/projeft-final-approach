<configuration>
    <dllmap dll="opengl32.dll">
        <dllentry os="linux" dll="libGL.so" />
        <dllentry os="osx" dll="/System/Library/Frameworks/OpenGL.framework/OpenGL" />
    </dllmap>
    <dllmap dll="lib/glfw.dll">
	  	<dllentry os="linux" dll="libglfw.so" />
  		<dllentry os="osx" cpu="x86" dll="lib/libglfw-32.dylib" />
  		<dllentry os="osx" cpu="x86-64" dll="lib/libglfw-64.dylib" />
    </dllmap>
	<dllmap dll="lib/fmodex.dll">
		<dllentry os="linux" dll="libfmodex-4.34.05.so"/>
		<dllentry os="osx" dll="lib/libfmodex.dylib"/>
	</dllmap>
	<dllmap dll="lib/soloud.dll">
		<dllentry os="osx" cpu="x86" dll="lib/libsoloud-32.dylib"/>
		<dllentry os="osx" cpu="x86-64" dll="lib/libsoloud-64.dylib"/>
	</dllmap>
</configuration>
